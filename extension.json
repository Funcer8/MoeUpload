{
	"name": "MoeUpload",
	"author": "March, nybux.tsui, XpAhH, baskice, dreamnong and AnnAngela",
	"url": "https://github.com/moegirlwiki/MoeUpload",
	"version": "1.3.0",
	"descriptionmsg": "moeupload-dec",
	"license-name": "BSD-2-Clause",
	"type": "parserhook",
	"MessagesDirs": {
		"MoeUpload": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"MoeUploadHooks": "includes/MoeUpload.php"
	},
	"Hooks": {
		"UploadFormInitDescriptor": [
			"MoeUploadHooks::onUploadFormInitDescriptor"
		],
		"UploadForm:BeforeProcessing": [
			"MoeUploadHooks::onBeforeProcessing"
		],
		"UploadForm:initial": [
			"MoeUploadHooks::onUploadForm_initial"
		]
	},
	"ResourceModules": {
		"ext.MoeUpload": {
			"dependencies": [
				"mediawiki.jqueryMsg"
			],
			"scripts": [
				"MoeUpload.js"
			],
			"messages": [
				"moeupload-Moe",
				"moeupload-CharName",
				"moeupload-Author",
				"moeupload-SrcUrl",
				"moeupload-dec",
				"moeupload-ForManagementOnly",
				"moeupload-PageInsteadOfImg",
				"moeupload-SameAsSourceURL",
				"moeupload-NoFile",
				"moeupload-NoDetail",
				"moeupload-HaveSymbol"
			],
			"targets": [
				"desktop"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "MoeUpload/resources"
	},
	"manifest_version": 2
}
